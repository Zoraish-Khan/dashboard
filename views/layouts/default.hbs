<!doctype html>
<html lang="en" data-bs-theme="auto" data-layout="vertical" data-layout-width="fullSize">

<head>
    <meta charset="utf-8" />
    <title>Dashboard | Bootstrap 5</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="Themesbrand" name="author" />
    {{!--
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/simplebar@latest/dist/simplebar.css" /> --}}
    <!-- App favicon -->
    <link rel="shortcut icon" href="/img/favicon.ico">

    <link href="/css/app-main.css" rel="stylesheet" type="text/css" />

    {{!-- CUSTOM CSS IS LOADED HERE... --}}
    {{#each css}}
    <!-- {{this}} -->
    <link href="{{this}}" rel="stylesheet" type="text/css" />
    {{/each}}
</head>

<body>
    <div id="main-layout-wrapper">

        {{> header}}
        <!-- ========== App Menu ========== -->
        {{> sidebar}}
        <!-- Sidebar End -->

        <!-- ============================================================== -->
        <!-- Start Main Content here -->
        <!-- ============================================================== -->
        <div class="main-content-wrap">
            <div class="page-content">
                <div class="container-fluid">
                    {{{body}}}
                </div>
            </div>
            {{> footer}}
        </div>
        <!-- end main content-->

    </div>
    <!-- END layout-wrapper -->
    {{> control-settings}}
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.7/dist/umd/popper.min.js"
        integrity="sha384-zYPOMqeu1DAVkHiLqWBUTcbYfZ8osu1Nd6Z89ify25QV9guujx43ITvfi12/QExE"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.min.js"
        integrity="sha384-Y4oOpwW3duJdCWv5ly8SCFYWqFDsfob/3GkgExXKV4idmbt98QcxXYs9UoXAB7BZ"
        crossorigin="anonymous"></script>


    <script src="https://cdn.jsdelivr.net/npm/simplebar@latest/dist/simplebar.min.js"></script>

    <!-- App js -->
    <script src="/js/app.js"></script>

    {{!-- CUSTOM JS IS LOADED HERE --}}
    {{#each js}}
    <script src="{{this}}"></script>
    {{/each}}

</body>

</html>